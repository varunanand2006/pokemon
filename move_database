emoji_dict = {
    "clear": "",
    "sunny": "",
    "rainy": "",
    "sandstorm": "",
    "hail": "",
    "extreme sun": "",
    "extreme rain": "",
    "extreme winds": "",

    "physical": "",
    "special": "",
    "status": "-",
    "accuracy": "",

    "shiny": "",

    "normal": "",
    "fire": "",
    "water": "",
    "grass": "",
    "electric": "",
    "ice": "",
    "rock": "",
    "ground": "",
    "steel": "",
    "bug": "",
    "fairy": "",
    "poison": "",
    "fighting": "",
    "dark": "",
    "psychic": "",
    "ghost": "",
    "flying": "",
    "dragon": "",

    "attack": "",
    "block": "",
    "heal": ""

}

def print_symbols(list):
    return_list = []
    for item in list:
        return_list.append(emoji_dict[item])
    return return_list


