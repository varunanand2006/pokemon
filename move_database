from pokemon_pokedex import pokemon_pokedex
pokedex = pokemon_pokedex
def return_pokedex():
    def decode_advanced_pokedex():
        file = open("advanced_pokedex", "r")
        txt = file.read().split("\n")
        file.close()
        new_string = ""

        for i in txt:
            if i[0] != " ":
                new_string += "|"
            new_string += i
        list_of_poke = new_string.split("|")[1:]
        return_pokedex = {}
        for item in list_of_poke:
            if "chikorita" in item:
                break
            temp_list = item.split("    ")
            if temp_list[0][:-1][-5:] == "-mega":
                temp_list[0] = "mega " + temp_list[0][:-1][:-4]
            elif temp_list[0][:-1][-7:-2] == "-mega":
                temp_list[0] = "mega " + temp_list[0][:-1][:-7] + " {} ".format(temp_list[0][:-1][-1])
            name = temp_list[1][11:].lower()
            if temp_list[3][8:] != ":":
                name += temp_list[3][9:].lower()
            temp_list[0] = name + "."
            return_pokedex[temp_list[0][:-1]] = {
                "name": name,
                "generation": int(temp_list[4][5]),
                "stats": {"hp": int(temp_list[10][4:]), "attack": int(temp_list[12][8:]), "defense": int(temp_list[14][9:]),
                          "s_attack": int(temp_list[16][7:]), "s_defense": int(temp_list[18][7:]),
                          "speed": int(temp_list[20][7:])},
                "species": temp_list[21][9:].lower(),
                "height": (temp_list[22][8:]),
                "weight": temp_list[23][8:],
                "gender ratio": temp_list[24][9:][:-1],
                "catch rate": temp_list[25][12:],
                "base exp": temp_list[26][10:],
                "growth rate": temp_list[29][13:],
                "ev yield": temp_list[32:],
                "type": [temp_list[6][7:]]
            }
            ev_yield = {}
            for item in return_pokedex[temp_list[0][:-1]]["ev yield"]:
                if item != "" and item != "-":
                    if len(item.split(": ")) > 1:
                        ev_yield[item.split(": ")[0]] = int(item.split(": ")[1])
                    else:
                        ev_yield = {}
                        break
            return_pokedex[temp_list[0][:-1]]["ev yield"] = ev_yield
            if temp_list[7][7:] != "" and temp_list[7][7:] != "-":
                return_pokedex[temp_list[0][:-1]]["type"].append(temp_list[7][7:])

            if return_pokedex[temp_list[0][:-1]]["weight"] == "":
                return_pokedex[temp_list[0][:-1]]["weight"] = 0
            if return_pokedex[temp_list[0][:-1]]["catch rate"] == "":
                return_pokedex[temp_list[0][:-1]]["catch rate"] = 0
            if return_pokedex[temp_list[0][:-1]]["base exp"] == "":
                return_pokedex[temp_list[0][:-1]]["base exp"] = 0
            if return_pokedex[temp_list[0][:-1]]["gender ratio"] == "enderles":
                return_pokedex[temp_list[0][:-1]]["gender ratio"] = "genderless"
            return_pokedex[temp_list[0][:-1]]["height"] = float(return_pokedex[temp_list[0][:-1]]["height"])
            return_pokedex[temp_list[0][:-1]]["weight"] = float(return_pokedex[temp_list[0][:-1]]["weight"])
            return_pokedex[temp_list[0][:-1]]["catch rate"] = float(return_pokedex[temp_list[0][:-1]]["catch rate"])
            return_pokedex[temp_list[0][:-1]]["base exp"] = float(return_pokedex[temp_list[0][:-1]]["base exp"])
        return return_pokedex

    pokedex = decode_advanced_pokedex()
    #14, 15, 16

    def ability_decoder():
        ability = {}
        txt = open("computer_file", "r").read()
        for item in txt.split("\n"):
            #print(item.split("\t"))
            ability[item.split("\t")[2].lower()] = [item.split("\t")[14]]
            if item.split("\t")[15] != "":
                ability[item.split("\t")[2].lower()].append(item.split("\t")[15])
            if item.split("\t")[16] != "":
                ability[item.split("\t")[2].lower()].append(item.split("\t")[16])
        #print(ability)
        return ability

    ability_dex = ability_decoder()

    for item in pokedex:
        try:
            pokedex[item]["ability"] = ability_dex[item]
        except:
            pokedex[item]["ability"] = [""]



    return pokedex





#print(return_pokedex())

